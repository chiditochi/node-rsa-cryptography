{
  "name": "rsa-cryptography",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "debug": "ts-node src/index.ts",
    "dev:watch": "tsc && node dist/index.js",
    "dev": "npm run build && npm run start",
    "clean": "rm -rf dist/ keys/ node-rsa-cryptography/ node-rsa-cryptography.zip",
    "build": "npm run clean && tsc --skipLibCheck",
    "start": "node dist/index.js",
    "zip": "zip -r node-rsa-cryptography.zip . -x '*node_modules/*' '*node_modules' 'dist/*' 'keys/*'"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/node": "^24.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "@types/node-forge": "^1.3.14"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "node-forge": "^1.3.1"
  },
  "module": "src/index.ts",
  "private": true
}
